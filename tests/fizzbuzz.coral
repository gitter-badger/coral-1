extern "C" printf : Fn[..., Void]

func fizzbuzz : Void(n: Int64):
  if n % 15 = 0: printf "fizzbuzz"
  elif n % 3 = 0: printf "fizz"
  elif n % 5 = 0: printf "buzz"
  else printf("%d", n)
        
func loop(n: Int64, m: Int64):
  if n = m:
    fizzbuzz n
  else:
    fizzbuzz n
    printf " "
    loop(n + 1, m)
    
func main():
  loop(1, 16)
