type Token:
  | NumberLiteral (n : String)
  | StringLiteral (s : String)
  | Identifier (name : String)
  | Operator (op : String)
  | Comment (val : String)
  | Indent
  | Dedent
  | Newline
  | Whitespace
  | Other (Char)

let x = NumberLiteral "456"
let y = NumberLiteral 3
let z = NumberLiteral "55"

type AstNode:
  | NumberLiteral (n)
  | StringLiteral (s)
  | FunctionDef
  | Identifier
