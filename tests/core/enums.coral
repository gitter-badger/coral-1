let _expected = "Friday\nSaturday\nSadderday\n"

type DayOfWeek:
  | Friday
  | Saturday
  | Other

func show(d : DayOfWeek):
  match d:
    Friday:
      printf "Friday\n"
    Saturday:
      printf "Saturday\n"
    Other:
      printf "Sadderday\n"

let x = DayOfWeek.Friday
let y = DayOfWeek.Saturday
show x
show y
show (DayOfWeek.Other)