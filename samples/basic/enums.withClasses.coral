type MyType:
  Foo
  Bar

func toString(m):
  match m:
    Foo: "oof"
    Bar: "rab"

impl MyType:
  func frob(this):
    match this:
      Foo: 1
      Bar: 2

class Num for T:
  zero: T
  add: Fn[T, T, T]
  mul: Fn[T, T, T]
  
impl Num for MyType:
  let zero = Foo
  func add(a, b):
     Bar
  func mul(a, b):
     Foo
     
class Show for T:
  tostr: Fn[T, String]

impl Show for MyType:
  func tostr(this): toString(this)
