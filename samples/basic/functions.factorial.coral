extern "C" printf : Fn[..., Void]

func factorial : Int64 (n : Int64):
    if n < 2:
        return 1
    else:
        return n * factorial (n - 1)

func factorial2 : Int64 (n : Int64, m : Int64):
    if n < 2:
        return m
    else:
        return factorial2 (n - 1, m * n)

func main : Int32 ( ) :
     printf("%d! = %d\n", 10, factorial 10)
     printf("%d! = %d\n", 10, factorial2(10, 1))
     return 0
